<kiyaa-taglib>
<!--
	This tag library contains tags which implement the View interface.
	
	The classes inside the widgets package are wrapper for the GWT
	widgets which implement the View interface and/or provide helpful
	setters where the object can't sufficiently be configured using
	attributes. 
 -->
	<namespace>http://habitsoft.com/kiyaa/ui</namespace>
	<package>com.habitsoft.kiyaa</package>
	<tag>
		<tag-name>custom</tag-name>
		<tag-class>views.View</tag-class>
	</tag>
	<tag>
		<tag-name>when</tag-name>
		<tag-class>views.WhenView</tag-class>
	</tag>
	<tag>
		<tag-name>list</tag-name>
		<tag-class>views.ListView</tag-class>
	</tag>
	<tag>
		<tag-name>table</tag-name>
		<tag-class>views.TableView</tag-class>
	</tag>
    <tag>
        <tag-name>css-table</tag-name>
        <tag-class>views.CssTableView</tag-class>
    </tag>
	<tag>
		<tag-name>button</tag-name>
		<tag-class>widgets.Button</tag-class>
		<content-attribute>text</content-attribute>
	</tag>
	<tag>
		<tag-name>label</tag-name>
		<tag-class>widgets.Label</tag-class>
		<content-attribute>text</content-attribute>
	</tag>
	<tag>
		<tag-name>listbox</tag-name>
		<tag-class>widgets.ListBox</tag-class>
	</tag>
	<tag>
		<tag-name>textbox</tag-name>
		<tag-class>widgets.TextBox</tag-class>
        <content-attribute>text</content-attribute>
	</tag>
    <tag>
        <tag-name>passwordtextbox</tag-name>
        <tag-class>widgets.PasswordTextBox</tag-class>
        <content-attribute>text</content-attribute>
    </tag>
	<tag>
		<tag-name>suggestbox</tag-name>
		<tag-class>widgets.SuggestBox</tag-class>
	</tag>
	<tag>
		<tag-name>textarea</tag-name>
		<tag-class>widgets.TextArea</tag-class>
        <content-attribute>text</content-attribute>
	</tag>
    <tag>
        <tag-name>richtextarea</tag-name>
        <tag-class>widgets.RichTextArea</tag-class>
        <content-attribute>text</content-attribute>
    </tag>
	<tag>
		<tag-name>mappedlabel</tag-name>
		<tag-class>widgets.MappedLabel</tag-class>
	</tag>
	<tag>
		<tag-name>editablelabel</tag-name>
		<tag-class>widgets.EditableLabel</tag-class>
	</tag>
	<tag>
		<tag-name>editablelabellistbox</tag-name>
		<tag-class>widgets.EditableLabelListBox</tag-class>
	</tag>
	<tag>
		<tag-name>editablelabeltextarea</tag-name>
		<tag-class>widgets.EditableLabelTextArea</tag-class>
	</tag>
	<tag>
		<tag-name>dock</tag-name>
		<tag-class>widgets.DockPanel</tag-class>
	</tag>
	<tag>
		<tag-name>deck</tag-name>
		<tag-class>widgets.DeckPanel</tag-class>
	</tag>
	<tag>
		<tag-name>customcombobox</tag-name>
		<tag-class>views.CustomComboBox</tag-class>
	</tag>
	<tag>
		<tag-name>two-column-form</tag-name>
		<tag-class>forms.TwoColumnForm</tag-class>
	</tag>
    <tag>
        <tag-name>two-row-form</tag-name>
        <tag-class>forms.TwoRowForm</tag-class>
    </tag>
    <tag>
        <tag-name>vertical-form</tag-name>
        <tag-class>forms.VerticalForm</tag-class>
    </tag>
    <tag>
        <tag-name>dynamicview</tag-name>
        <tag-class>views.DynamicView</tag-class>
    </tag>
    <tag>
        <tag-name>hyperlink</tag-name>
        <tag-class>widgets.Hyperlink</tag-class>
        <content-attribute>text</content-attribute>
    </tag>
    <tag>
        <tag-name>date-picker</tag-name>
        <tag-class>widgets.DatePicker</tag-class>
        <content-attribute>date</content-attribute>
    </tag>
    <tag>
        <tag-name>percentage</tag-name>
        <tag-class>widgets.PercentageTextBox</tag-class>
        <content-attribute>value</content-attribute>
    </tag>
    <tag>
        <tag-name>shadedborder</tag-name>
        <tag-class>widgets.ShadedBorder</tag-class>
    </tag>
    <tag>
        <tag-name>feedback</tag-name>
        <tag-class>widgets.Feedback</tag-class>
        <content-attribute>text</content-attribute>
    </tag>
    <tag>
        <tag-name>a</tag-name>
        <tag-class>widgets.Anchor</tag-class>
        <content-attribute>text</content-attribute>
    </tag>
    <tag>
        <tag-name>a-span</tag-name>
        <tag-class>widgets.AnchorWithSpan</tag-class>
        <content-attribute>text</content-attribute>
    </tag>
    <tag>
        <tag-name>booleanDropDown</tag-name>
        <tag-class>widgets.BooleanDropDown</tag-class>
        <content-attribute>booleanValue</content-attribute>
    </tag>
</kiyaa-taglib>
