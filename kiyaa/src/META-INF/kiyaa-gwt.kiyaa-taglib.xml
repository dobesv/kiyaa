<kiyaa-taglib>
	<!--
		This tag library contains GWT widgets that can be instantiated
		directly using the already available attributes. 
	-->
	<namespace>http://habitsoft.com/kiyaa/gwt</namespace>
	<package>com.google.gwt.user.client.ui</package>
	<tag>
		<tag-name>textbox</tag-name>
		<tag-class>TextBox</tag-class>
		<content-attr>text</content-attr>
	</tag>
    <tag>
        <tag-name>passwordtextbox</tag-name>
        <tag-class>PasswordTextBox</tag-class>
        <content-attr>text</content-attr>
    </tag>
	<tag>
		<tag-name>textarea</tag-name>
		<tag-class>TextArea</tag-class>
		<content-attribute>text</content-attribute>
	</tag>
	<tag>
		<tag-name>label</tag-name>
		<tag-class>Label</tag-class>
		<content-attribute>text</content-attribute>
	</tag>
    <tag>
        <tag-name>html</tag-name>
        <tag-class>HTML</tag-class>
        <content-attribute>hTML</content-attribute>
    </tag>
	<tag>
		<tag-name>listbox</tag-name>
		<tag-class>ListBox</tag-class>
	</tag>
	<tag>
		<tag-name>dialog</tag-name>
		<tag-class>DialogBox</tag-class>
	</tag>
	<tag>
		<tag-name>flextable</tag-name>
		<tag-class>FlexTable</tag-class>
	</tag>
	<tag>
		<tag-name>grid</tag-name>
		<tag-class>Grid</tag-class>
	</tag>
	<tag>
		<tag-name>dock</tag-name>
		<tag-class>DockPanel</tag-class>
	</tag>
    <tag>
        <tag-name>simplepanel</tag-name>
        <tag-class>SimplePanel</tag-class>
    </tag>
    <tag>
        <tag-name>flowpanel</tag-name>
        <tag-class>FlowPanel</tag-class>
    </tag>
    <tag>
        <tag-name>hyperlink</tag-name>
        <tag-class>Hyperlink</tag-class>
    </tag>
    <tag>
        <tag-name>checkbox</tag-name>
        <tag-class>CheckBox</tag-class>
        <content-attribute>text</content-attribute>
    </tag>
    <tag>
        <tag-name>span</tag-name>
        <tag-class>InlineHTML</tag-class>
        <content-attribute>text</content-attribute>
    </tag>
    <tag>
        <tag-name>a</tag-name>
        <tag-class>Anchor</tag-class>
        <default for="href">javascript:void(0)</default>
        <content-attribute>text</content-attribute>
    </tag>
    <tag>
        <tag-name>form</tag-name>
        <tag-class>FormPanel</tag-class>
        <default for="encoding">multipart/form-data</default>
        <default for="method">post</default>
    </tag>
    <tag>
        <tag-name>file-upload</tag-name>
        <tag-class>FileUpload</tag-class>
        <default for="name">data</default>
    </tag>
    <tag>
        <tag-name>img</tag-name>
        <tag-class>Image</tag-class>
        <content-attribute>url</content-attribute>
    </tag>
    <tag>
        <tag-name>button</tag-name>
        <tag-class>Button</tag-class>
        <content-attribute>text</content-attribute>
    </tag>
    <tag>
        <tag-name>hidden</tag-name>
        <tag-class>Hidden</tag-class>
        <content-attribute>value</content-attribute>
    </tag>
</kiyaa-taglib>